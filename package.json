	"private": true,
	"scripts": {
		"dev": "concurrently -i \"cross-env NODE_OPTIONS='--inspect' next dev\" \"npx nodemon src/server/socket.js\"",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"pushSupabaseDb": "npx prisma db push --schema=src/prisma/schema.prisma",
		"pullSupabaseDb": "npx prisma db pull --schema=src/prisma/schema.prisma",
		"prismaGenerate": "npx prisma generate --schema=src/prisma/schema.prisma",
		"pullAndGenerate": " npm run pullSupabaseDb && npm run prismaGenerate",
		"postinstall": "npm run prismaGenerate"
	},
